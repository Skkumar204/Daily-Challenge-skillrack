#include<stdio.h>
#include<stdlib.h>

int main()
{
    int n;
    
    int sum1=0,sum2=0;
    
    float t=0.0;
    
    while((scanf("%d ",&n))>0)
    {
        if(n%2==0)
        {
            sum1+=n;
        }
        if(n%2!=0)
        {
            t++;
            sum2+=n;
        }
    }

    if(sum1==0)
    {
        printf("-1");
    }
    else
    {
        printf("%d\n",sum1);
    }
    
    if(sum2==0)
    {
        printf("-1");
        printf("\n");
    }
    else
    {
        printf("%0.1f. ",sum2/t);
    }
    
}
